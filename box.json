{
  "directories": ["src/"],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        ".gitignore",
        ".md",
        "phpunit",
        "Tester",
        "Tests",
        "tests"
      ],
      "in": "vendor"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Herrera\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "git-commit": "package_version",
  "git-version": "git-version",
  "main": "vagrun",
  "output": "vagrun.phar",
  "stub": true,
  "chmod": "0755"
}
